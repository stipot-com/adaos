{
	"name": "inimatic",
	"description": "An Ionic project",
	"version": "0.0.1",
	"author": "STIpot",
	"contributors": [
		"Dmitry <connect@stipot.com>"
	],
	"homepage": "https://inimatic.com/",
	"eslintConfig": {
		"extends": "@ionic/eslint-config/recommended"
	},
	"prettier": {
		"files": [
			".prettierrc"
		]
	},
	"nodemonConfig": {
		"watch": [
			"backend"
		],
		"ext": "ts,json"
	},
	"type": "module",
	"scripts": {
		"ng": "ng",
		"ionic.serve": "ionic serve",
		"start": "ng serve --host 0.0.0.0",
		"build": "ionic build",
		"builddev": "ionic build",
		"buildprod": "ionic build --prod",
		"buildlocal": "npx ng run app:build.local",
		"start:api-dev": "cross-env DOTENV_CONFIG_PATH=./backend/.env nodemon --exec node --no-warnings=ExperimentalWarning -r dotenv/config --loader ts-node/esm ./backend/app.ts",
		"build:api": "tsc -p ./backend/tsconfig.backend.json",
		"serve:api": "PRODUCTION=true node ./dist/out-tsc/app.js",
		"serve:api-dev": "tsc -p ./backend/tsconfig.backend.json && PORT=3030 node ./dist/out-tsc/app.js",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint",
		"add.android": "ionic cordova platform add android",
		"build.android": "ionic cordova build android --prod",
		"a": "ionic cordova run android -l --external --address 192.168.1.8 -p=3030",
		"dev.android": "ionic cordova run android -l --external",
		"add.plugin": "ionic cordova plugin add ./LoginWV",
		"update.plugin": "ionic cordova plugin remove cordova-plugin-loginwv && ionic cordova plugin add ./LoginWV"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.0.2",
		"@angular/common": "^17.0.2",
		"@angular/compiler": "^17.0.2",
		"@angular/core": "^17.0.2",
		"@angular/forms": "^17.0.2",
		"@angular/platform-browser": "^17.0.2",
		"@angular/platform-browser-dynamic": "^17.0.2",
		"@angular/router": "^17.0.2",
		"@angular/service-worker": "^17.3.7",
		"@ionic-native/in-app-browser": "^5.36.0",
		"@ionic/angular": "^8.0.0",
		"@ionic/cli": "^7.2.0",
		"@ionic/cordova-builders": "^11.0.0",
		"@tensorflow/tfjs": "^4.22.0",
		"adm-zip": "^0.5.12",
		"angularx-qrcode": "^17.0.0",
		"axios": "^1.7.2",
		"body-parser": "^1.20.2",
		"cordova": "^12.0.0",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"firebase-functions": "^5.0.1",
		"http-proxy-middleware": "^3.0.5",
		"ionicons": "^7.2.1",
		"jsqr": "^1.4.0",
		"jsqr-es6": "^1.4.0-1",
		"node-forge": "^1.3.1",
		"process": "^0.11.10",
		"redis": "^4.7.0",
		"rxjs": "~7.8.0",
		"simple-git": "^3.22.0",
		"simple-peer": "^9.11.1",
		"socket.io": "^4.8.0",
		"socket.io-client": "^4.8.0",
		"streamsaver": "^2.0.6",
		"tslib": "^2.3.0",
		"uuid": "^10.0.0",
		"zone.js": "~0.14.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.0.0",
		"@angular-eslint/builder": "^17.0.0",
		"@angular-eslint/eslint-plugin": "^17.0.0",
		"@angular-eslint/eslint-plugin-template": "^17.0.0",
		"@angular-eslint/schematics": "^17.0.0",
		"@angular-eslint/template-parser": "^17.0.0",
		"@angular/cli": "^17.0.0",
		"@angular/compiler-cli": "^17.0.2",
		"@angular/language-service": "^17.0.2",
		"@ionic/angular-toolkit": "^11.0.1",
		"@types/adm-zip": "^0.4.34",
		"@types/axios": "^0.14.0",
		"@types/body-parser": "^1.19.5",
		"@types/express": "^4.17.21",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^20.14.5",
		"@types/node-forge": "^1.0.5",
		"@types/qrcode": "^1.5.5",
		"@types/simple-peer": "^9.11.8",
		"@types/streamsaver": "^2.0.5",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"cordova-android": "^12.0.1",
		"cordova-plugin-android-permissions": "^1.1.5",
		"cordova-plugin-device": "2.1.0",
		"cordova-plugin-ionic-keyboard": "^2.0.5",
		"cordova-plugin-ionic-webview": "^5.0.0",
		"cordova-plugin-loginwv": "file:LoginWV",
		"cordova-plugin-splashscreen": "5.0.2",
		"cordova-plugin-statusbar": "4.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^17.2.2",
		"eslint": "^8.57.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsdoc": "^48.2.1",
		"eslint-plugin-prefer-arrow": "1.2.2",
		"jasmine-core": "~5.1.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"nodemon": "^3.1.10",
		"ts-node": "^10.9.2",
		"typescript": "~5.4.0"
	},
	"cordova": {
		"plugins": {
			"cordova-plugin-statusbar": {},
			"cordova-plugin-device": {},
			"cordova-plugin-splashscreen": {},
			"cordova-plugin-ionic-webview": {},
			"cordova-plugin-ionic-keyboard": {},
			"cordova-plugin-android-permissions": {},
			"cordova-plugin-loginwv": {}
		},
		"platforms": [
			"android"
		]
	}
}

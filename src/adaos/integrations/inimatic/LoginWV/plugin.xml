<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-loginwv" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>LoginWV</name>
	<js-module name="LoginWV" src="www/LoginWV.js">
		<clobbers target="LoginWV" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="LoginWV">
				<param name="android-package" value="inimatic.loginwv.LoginWV" />
			</feature>
		</config-file>
		<!-- <config-file parent="/*" target="AndroidManifest.xml"></config-file> -->
		<source-file src="src/android/LoginWV.java" target-dir="src/cordova-plugin-loginwv/" />
	</platform>
</plugin>
